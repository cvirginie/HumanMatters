    "df_scored['File Name'][0]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 319,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_job_number(val):\n",
    "    JobNumber = re.compile('\\D\\d+') # match job number\n",
    "    jn = re.search(JobNumber, val)\n",
    "    return jn.group().lstrip()\n",
    "\n",
    "df_scored['JobNumber'] = df_sorted['File Name'].apply(lambda val : get_job_number(str(val)))"
   ]
  },
